        "commitizen": "^4.2.5",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "^8.24.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.7.1",
        "typescript": "^4.8.4"
    },
    "config": {
